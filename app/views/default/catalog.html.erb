<% @categories.each do |category|%>

    <div class="list-group">
      <a href="/catalog/<%=category.id%>" class="list-group-item">
        <h4 class="list-group-item-heading"> <%=category.subject%></h4>
        <p class="list-group-item-text"><%=category.description%></p>
      </a>
    </div>


<% end %>